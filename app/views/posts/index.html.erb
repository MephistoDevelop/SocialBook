<% @posts.each do |post| %>
  <% user = User.where(id: post.user_id) %>
  <br>
  <%= post.content %> <br>
  <% user.each do |user| %>
    User: <%= user.username %>
  <% end %>
  <% post.comments.each do |comment| %>
    Comment: <%= comment.content %><br>
    <% end %>
  Reactions:
  <%= post.total_reactions %>
<% end %>
